datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  directUrl         = env("DIRECT_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Profile {
  id    String @id @default(uuid())
  clerkId  String @unique
  email String 
  firstName String
  lastName String
  profileImage String
  username String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}